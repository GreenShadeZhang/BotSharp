    "Assemblies": [ "BotSharp.Core" ]
  },
  "FileCore": {
    "Storage": "PostgreSqlFileStorage",
    "Pdf2TextConverter": {
      "Provider": ""
    },
    "Pdf2ImageConverter": {
      "Provider": ""
    }
  }
}
